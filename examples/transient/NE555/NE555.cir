//https://softsolder.com/2012/12/24/getting-20-duty-cycle-from-a-555-timer
test_NE555( R ) :=
{
  V1.CV( @3, @0, 7.5 );
  C1.CC( @1, @0, 1u );
  RB.CR( @2, @1, R);
  D2.DD( @1, @2 );
  RA.CR( @3, @2, 2.7K );
  IC.NE555( @1, @1, @2, @4, @3, @0  ); //@trigger,  @threshold,  @discharge, @output,  @vcc, @gnd
  P1.PROBE( @4, @0 )
};

CIRCUIT := RESOLVE_TRANS( test_NE555( 13K ) )
